# Disguise as IE 9 on Windows 7. May trigger fallback versions, making them easier to scrape
user-agent = "Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)"

# When following a redirect, set previous URL as referer. (to have valid referer for redirectins)
referer = ";auto"

# Wait 60 seconds before timing out. Prevents hanging indefinitely.
connect-timeout = 60
